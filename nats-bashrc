alias watch='watch '
alias nsr='nats server report jetstream --server=nats --user=${NATS_ADMIN_USER} --password=${NATS_ADMIN_PASSWORD}'
alias nci='nats consumer info EVENTS CONSUMER --server=nats --user=${NATS_USERNAME} --password=${NATS_PASSWORD}'
alias nsi='nats stream info EVENTS --server=nats --user=${NATS_USERNAME} --password=${NATS_PASSWORD}'